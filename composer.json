{
    "name": "tisuit/skeleton",
    "description": "Skeleton for TiSuit",
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "tisuit/core": "@stable",
        "vlucas/phpdotenv": "@stable",
        "hirak/prestissimo": "@stable"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src/"
        }
    },
    "config": {
        "vendor-dir": "app/vendor"
    },
    "scripts": {
        "post-create-project-cmd": [
            "rm -f CODE_OF_CONDUCT.md CONTRIBUTING.md LICENSE README.md .travis.yml"
        ]
    }
}
